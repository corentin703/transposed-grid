{
  "name": "transposed-grid",
  "version": "1.0.2",
  "description": "A Stencil JS based transposed grid / vertical grid",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dyarnist/transposed-grid/transposed-grid.esm.js",
  "homepage": "https://corentin703.github.io/transposed-grid",
  "repository": {
    "type": "git",
    "url": "https://github.com/corentin703/transposed-grid.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "lint": "eslint src/**/*{.ts,.tsx}",
    "storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006",
    "build-storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider build-storybook",
    "deploy-storybook": "gh-pages -d storybook-static"
  },
  "dependencies": {
    "@material/button": "^14.0.0",
    "@material/checkbox": "^14.0.0",
    "@material/data-table": "^14.0.0",
    "@material/icon-button": "^14.0.0",
    "@material/textfield": "^14.0.0",
    "@stencil/core": "^4.24.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@stencil-community/eslint-plugin": "^0.9.0",
    "@stencil/react-output-target": "^0.8.2",
    "@stencil/sass": "^3.0.12",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/html": "^6.5.16",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.0",
    "babel-loader": "^9.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.18.0",
    "eslint-plugin-storybook": "^0.11.2",
    "gh-pages": "^5.0.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^19.5.2"
  },
  "license": "MIT"
}
